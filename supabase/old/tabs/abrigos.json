{
  "module": "Abrigos & Estrutura",
  "tables": {
    "canis": {
      "columns": {
        "id": "bigint (PK, identity)",
        "canil": "text",
        "proprietario": "uuid (FK -> usuarios.user_id)",
        "excluido": "boolean (default false)",
        "aceita_inscricoes": "boolean",
        "logotipo_url": "text"
      }
    },
    "canis_membros": {
      "columns": {
        "id": "bigint (PK, identity)",
        "canil": "bigint (FK -> canis.id)",
        "membro": "uuid (FK -> usuarios.user_id)",
        "admin": "boolean (default false)",
        "guest": "boolean (default false)",
        "vet": "boolean (default false)",
        "solicitacao_aceita": "boolean (default false)",
        "solicitacao_realizada": "boolean (default false)"
      }
    },
    "canil_tratador": {
      "columns": {
        "id": "bigint (PK, identity)",
        "canil": "bigint (FK -> canis.id)",
        "usuario": "text (NOT NULL)",
        "nome": "text",
        "senha": "text"
      }
    },
    "clinicas": {
      "columns": {
        "clinica": "text (PK)",
        "telefone": "text",
        "endereco": "text",
        "logo": "text",
        "canil_id": "bigint (FK -> canis.id)"
      }
    },
    "cores": {
      "columns": {
        "cor": "text (PK)",
        "canil_id": "bigint (FK -> canis.id)"
      }
    },
    "especies": {
      "columns": {
        "especie": "text (PK)",
        "indice": "smallint",
        "canil_id": "bigint (FK -> canis.id)"
      }
    },
    "racas": {
      "columns": {
        "raca": "text (PK, unique)",
        "indice": "smallint",
        "especie": "text (FK -> especies.especie, default 'Cachorro')",
        "canil_id": "bigint (FK -> canis.id)"
      }
    },
    "portes": {
      "columns": {
        "porte": "text (PK)",
        "descritivo": "text",
        "indice": "smallint",
        "limite_peso": "smallint"
      }
    },
    "pelagens": {
      "columns": {
        "pelagem": "text (PK)",
        "indice": "smallint"
      }
    },
    "idades": {
      "columns": {
        "idade": "text (PK, default '')",
        "indice": "smallint (default 0)",
        "descritivo": "text"
      }
    },
    "moradia_tipos": {
      "columns": {
        "tipo_moradia": "text (PK)"
      }
    }
  }
}
